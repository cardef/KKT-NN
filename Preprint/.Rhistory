library(tidyverse)
feas_df <- read_csv("plots/ineq.csv")%>%rename(Feasibility = Value)
stat_df <- read_csv("plots/stat.csv")%>%rename(Stationary = Value)
compl_df <- read_csv("plots/compl.csv")%>%rename(Complementary = Value)
df <- stat_df%>%inner_join(feas_df) %>% inner_join(compl_df) %>%select(!'Wall time') %>% pivot_longer(!Step, names_to="Loss", values_to="Value")
df
ggline(df, x="Step", y="Value", plot_type="l", color='Loss', ggtheme=theme_light()) + yscale("log10")
