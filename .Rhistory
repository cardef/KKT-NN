plt.plot(compl['Step'], compl['Value'], label = "$L_C$")
plt.plot(ineq['Step'], ineq['Value'], label = "$L_I$")
plt.legend(["$\\mathcal{L}_S$", "$\\mathcal{L}_C$", "$\\mathcal{L}_I$"], loc="upper center")
plt.tight_layout()
plt.xlabel("Step")
plt.ylabel("MAE")
plt.show())--")
